// Place your key bindings in this file to overwrite the defaults
[
  // Duplicate line, moving cursor down
  {
    "key": "ctrl+shift+d",
    "command": "editor.action.copyLinesDownAction",
    "when": ""
  },
  // Colapse file explorer folders
  {
    "key": "ctrl+e ctrl+c",
    "command": "workbench.files.action.collapseFilesExplorerFolders"
  }

  // Commonly used shortcuts:
  // ctrl+': toggle Terminal

]
